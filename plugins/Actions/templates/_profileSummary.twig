{% if visitorData.totalPageViews > 0 %}
    <div class="visitor-profile-summary visitor-profile-pages">
        <h1>{{ 'Live_TopVisitedPages'|translate }}</h1>
        <div>
            <p>
                {% if visitorData.totalRevisitedPages > 0 %}
                <strong>{{ visitorData.totalPageViews }} {{ 'General_ColumnPageviews'|translate }}</strong> ({{ visitorData.totalUniquePageViews }} {{ 'General_ColumnUniquePageviews'|translate }}, {{ visitorData.totalRevisitedPages }} {{ 'Live_RevisitedPages'|translate }})
                {% else %}
                    <strong>{{ visitorData.totalPageViews }} {{ 'General_ColumnUniquePageviews'|translate }}</strong>
                {% endif %}
            </p>
            <ul>
                {% for url,count in visitorData.visitedPages %}
                    {% if loop.index < 6 and count > 1 %}
                        <li><span class="repeat icon-refresh" title="{{ 'Live_PageRefreshed'|translate }}">{{ count }}</span> {{ url }}</li>
                    {% endif %}
                {% endfor %}
            </ul>
        </div>
    </div>
{% endif %}